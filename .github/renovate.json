{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": ["major", "minor", "patch", "pin"],
      "automerge": false
    }
  ],
  "labels": ["dependencies"],
  "gradle": {
    "fileMatch": ["(^|\/)gradle\/.+\.toml$", "\.versions\.toml$"]
  }
}
